    "relation": {
        "mitigates": [
            {
                "name": "Encrypt Sensitive Information",
                "description": "Protect sensitive information with strong encryption.",
                "type": "course-of-action",
                "id": "M1041"
            },
            {
                "name": "Remote Data Storage",
                "description": "Use remote security log and sensitive file storage where access can be controlled better to prevent exposure of intrusion detection log data or sensitive information.",
                "type": "course-of-action",
                "id": "M1029"
            },
            {
                "name": "Network Segmentation",
                "description": "Architect sections of the network to isolate critical systems, functions, or resources. Use physical and logical segmentation to prevent access to potentially sensitive systems and information. Use a DMZ to contain any internet-facing services that should not be exposed from the internal network. Configure separate virtual private cloud (VPC) instances to isolate critical cloud systems.",
                "type": "course-of-action",
                "id": "M1030"
            },
            {
                "name": "Restrict File and Directory Permissions",
                "description": "Restrict access by setting directory and file permissions that are not specific to users or privileged accounts.",
                "type": "course-of-action",
                "id": "M1022"
            }
        ]
    },
    "mitigationRequirements": {
        "requirements": [
            {
                "description": "Ensure that Data encryption process is implemented in the organization to prevent any modifications.",
                "acid": "98582756"
            },
            {
                "description": "Ensure that network segmentation is implemented to identify critical business processes and isolate them to protect against unauthorized access and tampering.",
                "acid": "98582757"
            },
            {
                "description": "Ensure that IT disaster recovery plans are implemented in the organization for taking regular data backups.",
                "acid": "98582758"
            },
            {
                "description": "Ensure that IT disaster recovery plans are implemented in the organization for testing regular data backups.",
                "acid": "98582759"
            },
            {
                "description": "Ensure that data backups are stored off system and protected from common adversaries methods.",
                "acid": "98582760"
            },
            {
                "description": "Ensure that least privilege principles are employed to important information resources to reduce data manipulation risk.",
                "acid": "98582761"
            }
        ]
    },
    "detectionRequirements": {
        "requirements": [
            {
                "description": "Ensure that monitoring solution is implemented to inspect important file hashes for suspicious/unexpected values.",
                "acid": "98582751"
            },
            {
                "description": "Ensure that monitoring solution is implemented to inspect locations for suspicious/unexpected values.",
                "acid": "98582752"
            },
            {
                "description": "Ensure that monitoring solution is implemented to inspect any modifications for suspicious/unexpected values.",
                "acid": "98582753"
            },
            {
                "description": "Ensure that system is configured to monitor for critical processes involve in transmission of data.",
                "acid": "98582754"
            },
            {
                "description": "Ensure that manual or out-of-band integrity checking is implemented to inspect manipulated data.",
                "acid": "98582755"
            }
        ]
    },
